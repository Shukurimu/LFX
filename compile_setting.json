{
  "Windows": {
    "path": "(YOU NEED TO SET IT MANUALLY)",
    "path_to_fx": "C:\\Program Files\\Java\\javafx-sdk-11\\lib"
  },
  "Linux": {
    "path": "${HOME}/jdk-11.0.2.jdk/Contents/Home/bin",
    "path_to_fx": "${HOME}/javafx-sdk-11.0.2/lib"
  },
  "destination": "classes",
  "root": "lfx",
  "steps": [
    {
      "target": "utils",
      "pathes": [
        "lfx/util/Area.java",
        "lfx/util/Const.java",
        "lfx/util/ElementSelector.java",
        "lfx/util/Point.java",
        "lfx/util/Selectable.java",
        "lfx/util/Tuple.java",
        "lfx/util/UnionReadOnlyIterable.java",
        "lfx/util/Util.java"
      ]
    },
    {
      "target": "image-util",
      "modules": ["javafx.graphics"],
      "pathes": [
        "lfx/util/ImageCell.java"
      ]
    },
    {
      "target": "bases",
      "pathes": [
        "lfx/base/Action.java",
        "lfx/base/Box.java",
        "lfx/base/Controller.java",
        "lfx/base/Cost.java",
        "lfx/base/Direction.java",
        "lfx/base/Order.java",
        "lfx/base/Scope.java",
        "lfx/base/Type.java",
        "lfx/base/Viewer.java"
      ]
    },
    {
      "target": "components",
      "pathes": [
        "lfx/component/Bdy.java",
        "lfx/component/Cpoint.java",
        "lfx/component/DataCollector.java",
        "lfx/component/Effect.java",
        "lfx/component/Frame.java",
        "lfx/component/Itr.java",
        "lfx/component/Opoint.java",
        "lfx/component/State.java",
        "lfx/component/Wpoint.java"
      ]
    },
    {
      "target": "base interfaces",
      "pathes": [
        "lfx/game/Environment.java",
        "lfx/game/Observable.java",
        "lfx/game/Playable.java"
      ]
    },
    {
      "target": "extend interfaces",
      "pathes": [
        "lfx/game/Hero.java",
        "lfx/game/Weapon.java",
        "lfx/game/Energy.java"
      ]
    },
    {
      "target": "integration interfaces",
      "pathes": [
        "lfx/game/Library.java",
        "lfx/game/Field.java"
      ]
    },
    {
      "target": "root object",
      "pathes": [
        "lfx/game/object/AbstractObject.java"
      ]
    },
    {
      "target": "base map",
      "modules": ["javafx.graphics"],
      "pathes": [
        "lfx/game/object/AbstractField.java"
      ]
    },
    {
      "target": "base objects",
      "pathes": [
        "lfx/game/object/BaseHero.java",
        "lfx/game/object/BaseWeapon.java",
        "lfx/game/object/BaseEnergy.java"
      ]
    },
    {
      "target": "keyboard",
      "pathes": [
        "lfx/setting/Keyboard.java"
      ]
    },
    {
      "target": "selector functional",
      "pathes": [
        "lfx/platform/PlayableSelector.java"
      ]
    },
    {
      "target": "UIs",
      "modules": ["javafx.graphics"],
      "pathes": [
        "lfx/platform/KeyboardController.java",
        "lfx/map/StatusBoard.java"
      ]
    },
    {
      "target": "scenes",
      "modules": ["javafx.graphics", "javafx.controls"],
      "pathes": [
        "lfx/platform/GuiScene.java",
        "lfx/platform/ConfigScene.java",
        "lfx/platform/VersusArena.java",
        "lfx/platform/PlayerCard.java",
        "lfx/platform/PickingPhase.java",
        "lfx/platform/PickingScene.java"
      ]
    },
    {
      "target": "main",
      "modules": ["javafx.graphics", "javafx.controls"],
      "pathes": [
        "lfx/platform/Main.java"
      ]
    },
    {
      "target": "base tools",
      "pathes": [
        "lfx/tool/Extractor.java"
      ]
    },
    {
      "target": "component tools",
      "pathes": [
        "lfx/tool/BdyExtractor.java",
        "lfx/tool/CpointExtractor.java",
        "lfx/tool/ItrExtractor.java",
        "lfx/tool/OpointExtractor.java",
        "lfx/tool/WpointExtractor.java"
      ]
    },
    {
      "target": "tools",
      "pathes": [
        "lfx/tool/FrameExtractor.java",
        "lfx/tool/Parser.java"
      ]
    }
  ]
}
